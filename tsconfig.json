{
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		],
		"strictNullChecks": true,
		"baseUrl": "./src/",
		"paths": {}
	},
	"include": [
		"next-env.d.ts",
		".next/types/**/*.ts",
		"**/*.ts",
		"**/*.tsx",
		"src/app/apartament/[slug]/components/Apartament.tsx",
		"src/app/signIn/page.tsx",
		"src/lib/auth.ts",
		"src/app/middleware.js"
, "src/app/components/LatestOffers.tsx"	],
	"exclude": ["node_modules"]
}
